    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<% if (typeof alert != "undefined") { %>
<script>
    Swal.fire({
        title: '<%= alertTitle %>',
        text: '<%= alertMenssage %>',
        icon: '<%= alertIcon %>',
        showConfirmButton: '<%= showConfirmButton%>',
        timer: '<%= timer%>'
    }).then((result) => {
      window.location.href = '/<%= ruta %>'
    });
</script>

<% } %>


