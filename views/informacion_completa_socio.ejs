          <span class="span_datos"><%= data.fechaNacimiento %></span><br>
        
          <p class="p_">Telefono:</p>
          <span class="span_datos"><%= data.socioTelefono %></span><br>
        
          <p class="p_">Direccion:</p>
          <span class="span_datos"><%= data.socioDireccion %></span><br>
        
          <p class="p_">Correo Electronico:</p>
          <span class="span_datos"><%= data.socioCorreoElectronico %></span><br>
        
          <p class="p_">Sexo:</p>
          <span class="span_datos"><%= data.sexo %></span><br>
        
          <p class="p_">Estado Civil:</p>
          <span class="span_datos"><%= data.estadoCivil %></span><br>
           
          <p class="p_">Fecha Registro:</p>
          <span class="span_datos"><%= data.socioFechaRegistro %></span><br>


