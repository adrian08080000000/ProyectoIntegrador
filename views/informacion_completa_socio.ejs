          <p class="p_">direccion empresa:</p>
          <span class="span_datos"><%= data.direccionEmpresa %></span><br>

          <p class="p_">Ganancias Mensual:</p>
          <span class="span_datos"><%= data.gananciasMensual %></span><br>
           
          <p class="p_">Fecha registro:</p>
        <span class="span_datos"><%= data.socioFechaRegistro %></span><br>

