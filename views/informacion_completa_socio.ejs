          <h1 style=" display:inline;">Informaci√≥n del socio, <%= data.socioNombre %>!</h1>
           <p >Datos privados</p> 
        </div>                              
        <br>  
       
        <p class="p_">Identificador del Socio</p>
        <span class="span_datos" style="color: rgb(177, 52, 52);"><%= data.idSocio %></span> <br>
        
        <p class="p_">Actividad</p>
        <span class="span_datos" ><%= data. activo_inactivo %></span> <br>
        

